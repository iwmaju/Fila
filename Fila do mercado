fila = ["Serena", "Maya", "Victor", "Majú"]


while True :    
    atendente = str(input("Digite um nome:"))
    cliente = str(input("Deseja finalizar sua lista? (sim ou não):"))
    fila.append(atendente)
    if cliente == "sim":
        break
    if atendente in fila:
        fila.pop(0)
print(fila)
